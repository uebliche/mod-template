{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": ${authors},
    "contact": ${contact},
    "license": "${license_type}",
    "icon": "${icon_path}",
    "environment": "*",
    "entrypoints": {
        "main": [
            "${main_class}"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json"
    ],
    "depends": {
        "fabricloader": "~${fabricloader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=21",
        "fabric-api": "*"
    },
    "custom": ${custom}
}