{
  "defaults": {
    "loader": "fabric",
    "runMode": "client",
    "version": "1.21.11"
  },
  "gradleArgs": [
    "--build-cache",
    "--parallel",
    "-PmcVersion={mcVersion}",
    "-Ploader_cache_file={loader_cache_file}",
    "-PonlyLoader={loader}"
  ],
  "gradleArgsByLoader": {
    "fabric": [
      "-PskipNeoForge=true"
    ],
    "neoforge": [
      "-PforceNeoForge=true"
    ],
    "paper": [
      "-PskipNeoForge=true"
    ],
    "velocity": [
      "-PskipNeoForge=true"
    ]
  },
  "group": "uebliche/mcmeta",
  "id": "template/mod-template",
  "kind": "project",
  "loaderSource": "mcmeta",
  "modrinth": {
    "projectId": "xMkfPrwZ"
  },
  "name": "Mod Template",
  "runModes": [
    "client",
    "server"
  ],
  "serverOnlyLoaders": [
    "paper",
    "velocity"
  ],
  "tasks": {
    "build": "build",
    "runClient": "runClient",
    "runServer": "runServer",
    "template": ":loader-{loader}:{task}"
  }
}
